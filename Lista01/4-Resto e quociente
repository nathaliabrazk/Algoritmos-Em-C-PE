//algoritmo que a partir da leitura de dois números forneça o resto e o
quociente da divisão do primeiro pelo segundo número.
#include<stdio.h>
int main(){
    int valorA, valorB, valorDivisao, restoDivisao;
	printf("Digite o valor A:  ");
	scanf("%d", &valorA);
	
	printf("Digite o valor B:  ");
	scanf("%d", &valorB);
	
	valorDivisao = valorA / valorB;
	restoDivisao = valorA % valorB;
	printf("Divisao: %d\n", valorDivisao);
	printf("Resto: %d\n", restoDivisao);

    return 0;
}
